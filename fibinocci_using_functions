import java.util.*;

class Solution {
    int fib(int n)
    {
        if(n==0)
        return 0;
        else if(n==1)
        return 1;
        else
        return n*fib(n-1);
    }

    public static void main(String[] args) {
        Solution obj = new Solution();
        Scanner sc = new Scanner(System.in);
    
        int n=sc.nextInt();
        System.out.println(obj.fib(n));
    }}
