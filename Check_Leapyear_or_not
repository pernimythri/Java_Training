import java.util.*;
class leap_Year {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in); 
      System.out.print("Enter year : ");
      int n=sc.nextInt();
      if(n%4==0 && ((n%100!=0 || n%400==0)))
       System.out.println("Leap year");
      else
       System.out.println("Not a Leap year");
}}
