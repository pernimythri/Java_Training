import java.util.*;
class Sum_Of_Digits {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in); 
      String s=sc.next();
     int count=0;
     for(char c :s.toCharArray())
     {
         count+=1;
     }
     
      System.out.println("No.of_Digits in a number : " + count);
    }}
