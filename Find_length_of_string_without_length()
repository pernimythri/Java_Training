import java.util.*;
class find_Length_Of_String {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in); 
      String s=sc.next();
     int count=0;
     for(char c :s.toCharArray())
     {
         count+=1;
     }
     
      System.out.println("Length : " + count);
    }}
