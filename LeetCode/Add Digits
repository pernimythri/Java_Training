import java.util.*;
class Main {
    public int addDigits(int num) {
    return 1 + (num-1)%9;
    }
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        Main obj=new Main();
        System.out.println(obj.addDigits(num));
    }
}
