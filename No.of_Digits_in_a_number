import java.util.*;
class Sum_Of_Digits {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in); 
      System.out.println("Enter a number ");
      int num=sc.nextInt();
     int count=0;
      for(;num!=0;num/=10)
      {
          count+=1;
      }
      
      System.out.println("No.of_Digits in a number : " + count);
    }}
