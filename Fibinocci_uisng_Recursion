import java.util.*;
class Fibinocci
{
    int  fib(int n)
    {
        if(n==0)
        return 0;
        else if(n==1)
        return 1;
        else
        return fib(n-1)+fib(n-2);
    }}
    class Main{
    public static void main(String[] args)
    {
        Fibinocci obj=new Fibinocci();
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter n value : ");
        int n=sc.nextInt();
        System.out.println("fibinocci number : " + obj.fib(n));
        
    }}
