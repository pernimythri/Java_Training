import java.util.*;
class Swap
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter a value ");
        int a=sc.nextInt();
        System.out.print("Enter b value ");
        int b=sc.nextInt();
        System.out.println("Before Swapping a = " + a +" "+ "b = " + b);
        a=a+b;
        b=a-b;
        a=a-b;
      System.out.println("Before Swapping a = " + a +" "+"b = " +b);    
    }
}
