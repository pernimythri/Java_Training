import java.util.*;
class Solution {
     boolean isPerfectSquare(int n)
    {
        int value=(int)Math.sqrt(n);
       if((value*value)==n) 
       return true;
       else
       return false;
    }
    

    public static void main(String[] args) {
        Solution obj = new Solution();
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println("Perfect Square  :" + obj.isPerfectSquare(n));
    }
}
