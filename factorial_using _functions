import java.util.*;

class Solution {
    int factorial(int n)
    {
        if (n==1)
        return 1;
        else
        return n*factorial(n-1);
    }

    public static void main(String[] args) {
        Solution obj = new Solution();
        Scanner sc = new Scanner(System.in);
    
        int n=sc.nextInt();
        System.out.println(obj.factorial(n));
    }}
